{
  "name": "sportradenenzo",
  "version": "0.3.0",
  "description": "Poulewebsite voor de beste poulegroep",
  "engines": {
    "node": "16.x",
    "npm": "6.x"
  },
  "repository": {
    "url": "https://github.com/RensReus/Sportradenenzo"
  },
  "dependencies": {
    "async": "^3.2.2",
    "bcrypt-nodejs": "0.0.3",
    "cheerio": "^1.0.0-rc.6",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.3",
    "fs-extra": "^9.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.2",
    "morgan": "^1.10.0",
    "node-schedule": "^1.3.3",
    "nodemailer": "^6.6.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "tslib": "^2.2.0"
  },
  "scripts": {
    "start": "npm run install-all && concurrently \"npm run server-dev\" \"npm run client\"",
    "server": "node build/server.js",
    "server-dev": "nodemon --config \"nodemon.json\"/",
    "client": "node start-client.js",
    "lint": "eslint .",
    "install-all": "npm install && cd .\\src\\client\\ && npm install --legacy-peer-deps && cd ..\\..",
    "build": "npm i --only=dev && npx -p typescript tsc && cd src/client/ && npm i --only=dev && npm i --save @babel/runtime && npm run build && cd ../..  && node heroku-build.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.41",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "ts-node": "^10.8.2",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  }
}
