{
  "name": "sportradenenzo",
  "version": "0.3.0",
  "description": "Poulewebsite voor de beste poulegroep",
  "repository": {
    "url": "https://github.com/RensReus/Sportradenenzo"
  },
  "dependencies": {
    "async": "^3.2.0",
    "bcrypt-nodejs": "0.0.3",
    "cheerio": "^1.0.0-rc.6",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node-schedule": "^1.3.3",
    "nodemailer": "^6.6.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "tslib": "^2.2.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server-dev\" \"npm run client\"",
    "start-dev": "echo \"This command has been deprecated. Use 'npm start'\" && concurrently \"npm run server-dev\" \"npm run client\"",
    "server": "node build/server.js",
    "server-dev": "nodemon --config \"nodemon.json\"/",
    "client": "node start-client.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "lint": "eslint .",
    "build": "npm i --only=dev && npx -p typescript tsc && cd src/client/ && npm i --only=dev && npm i --save @babel/runtime && npm run build && cd ../..  && node heroku-build.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.41",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
