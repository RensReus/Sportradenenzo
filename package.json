{
  "name": "sportradenenzo",
  "version": "0.3.0",
  "description": "Poulewebsite voor de beste poulegroep",
  "repository": {
    "url": "https://github.com/RensReus/Sportradenenzo"
  },
  "dependencies": {
    "async": "^2.6.3",
    "babel-core": "^6.26.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "canvasjs": "^1.8.1",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "flash": "^1.1.0",
    "fs-extra": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "node-schedule": "^1.3.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1",
    "pg-pool": "^2.0.7",
    "request": "^2.88.0",
    "sql.js": "0.3.2",
    "textarea-caret": "^3.1.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run client\"",
    "server": "node build/server.js",
    "server-dev": "nodemon --config \"nodemon.json\"/",
    "client": "node start-client.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "lint": "eslint .",
    "build": "npm i --only=dev && npx tsc && cd src/client/ && npm i --only=dev && npm i --save @babel/runtime && npm run build && cd ../..  && node heroku-build.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.11",
    "concurrently": "3.1.0",
    "nodemon": "^1.19.3",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
